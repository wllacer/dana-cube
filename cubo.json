                        "name": "a√±o"
                    },
                    {
                        "case_sql": [
                            "case",
                            "when TO_CHAR($$1,'MM') in ('01','02','03') then TO_CHAR($$1,'YYYY') ||':'||'1' ",
                            "when TO_CHAR($$1,'MM') in ('04','05','06') then TO_CHAR($$1,'YYYY') ||':'||'2' ",
                            "when TO_CHAR($$1,'MM') in ('07','08','09') then TO_CHAR($$1,'YYYY') ||':'||'3' ",
                            "when TO_CHAR($$1,'MM') in ('10','11','12') then TO_CHAR($$1,'YYYY') ||':'||'4' ",
                            "end as $$2"
                        ],
                        "class": "c",
